{
  "name": "offline-package-server",
  "version": "0.2.0",
  "description": "A offline package server",
  "author": "mcuking",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export NODE_ENV=development && nest start",
    "start:dev": "export NODE_ENV=development && nest start --watch",
    "start:debug": "export NODE_ENV=development && nest start --debug --watch",
    "start:prod": "npm run build && node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/typeorm": "^10.0.0",
    "bsdiff-node": "^2.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "md5": "^2.3.0",
    "muter": "^0.7.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/md5": "^2.3.4",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.7",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.15",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^5.20.1",
    "typescript": "^5.2.2"
  }
}
